{
	"name": "1_Master_Pipeline",
	"properties": {
		"activities": [
			{
				"name": "execute_2_copy_hydration_file",
				"type": "ExecutePipeline",
				"dependsOn": [
					{
						"activity": "lkp_conf_json",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"pipeline": {
						"referenceName": "2_Hydration_file_movement",
						"type": "PipelineReference"
					},
					"waitOnCompletion": true,
					"parameters": {
						"sharepointFolder": {
							"value": "@variables('sharepointContainer')",
							"type": "Expression"
						},
						"hydrationFileName": {
							"value": "@variables('hydFileName')",
							"type": "Expression"
						}
					}
				}
			},
			{
				"name": "lkp_conf_json",
				"type": "Lookup",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"dataset": {
						"referenceName": "ds_adls",
						"type": "DatasetReference"
					}
				}
			}
		],
		"variables": {
			"sharepointContainer": {
				"type": "String",
				"defaultValue": "sharepoint"
			},
			"hydFileName": {
				"type": "String",
				"defaultValue": "HydrationFile.csv"
			}
		},
		"annotations": []
	}
}